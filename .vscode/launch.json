{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach", /** attach conecta o debug na aplicação que está rodando*/
      "protocol": "inspector", /** --inspect no package.json para inspencionar o código node*/
      "restart": true,
      "name": "debug",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}
